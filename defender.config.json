{
  "contract_inspector": {
    "enabled": true,
    "scan_directories": ["./src"],
    "exclude_rules": [
      "different-pragma-directives",
      "floating-pragma",
      "lack-of-security-contact",
      "pragma-spans-breaking-changes",
      "require-missing-message",
      "unchecked-keyword",
      "unused-function"
    ]
  },
  "dependency_checker": {
    "enabled": true
  }
}
